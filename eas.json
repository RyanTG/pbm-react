{
  "cli": {
    "version": ">= 0.40.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "ios": {
        "resourceClass": "m-medium",
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_API_URL": "https://www.pinballmap.com/api/v1"
      },
    },
    "developmentSim": {
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://www.pinballmap.com/api/v1"
      },
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "androidapk": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://www.pinballmap.com/api/v1"
      },
      "channel": "androidapk"
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": false,
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://www.pinballmap.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
